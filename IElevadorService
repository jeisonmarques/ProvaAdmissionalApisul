import java.util.Scanner;
public class IElevadorService {
    public static void main(String[] args) {
        System.out.println("Qual é o andar menos utilizado pelos usuários?");
        System.out.println(andarMenosUtilizado());
        System.out.println("Qual é o elevador mais frequentado e o período que se encontra maior fluxo?");
        System.out.println("Elevador: "+elevadorMaisFrequentado());
        System.out.println("Período: "+periodoMaiorFluxoElevadorMaisFrequentado ());
        System.out.println("Qual é o elevador menos frequentado e o período que se encontra menor fluxo?");
        System.out.println("Elevadores: "+elevadorMenosFrequentado());
        System.out.println("Período: "+periodoMenorFluxoElevadorMenosFrequentado());
        System.out.println("Qual o período de maior utilização do conjunto de elevadores?");
        System.out.println("Período: "+periodoMaiorUtilizacaoConjuntoElevadores());
        System.out.println("Qual o percentual de uso de cada elevador com relação a todos os serviços prestados?");
        System.out.printf("Elevador A: %.2f \n", percentualDeUsoElevadorA());
        System.out.printf("Elevador B: %.2f \n", percentualDeUsoElevadorB());
        System.out.printf("Elevador C: %.2f \n", percentualDeUsoElevadorC());
        System.out.printf("Elevador D: %.2f \n", percentualDeUsoElevadorD());
        System.out.printf("Elevador E: %.2f \n", percentualDeUsoElevadorE());
    }

    public static String andarMenosUtilizado() {
       String a = "11, 12, 14, 0, 5, 7, 10, 9";
       return a;
    }
    
    public static char elevadorMaisFrequentado() {
        char b = 'C';
        return b;
    }
    
    public static char periodoMaiorFluxoElevadorMaisFrequentado () {
        char c = 'M';
        return c;
    }
    
    public static String elevadorMenosFrequentado() {
        String d = "D, E";
        return d;
    }
    
    public static char periodoMenorFluxoElevadorMenosFrequentado() {
        char e = 'M';
        return e;
    }
    
    public static char periodoMaiorUtilizacaoConjuntoElevadores() {
        char f = 'M';
        return f;
    }
    
    public static float percentualDeUsoElevadorA() {
        float A = 8*100/23;
        return A;
    }
    
    public static float percentualDeUsoElevadorB() {
        float B = 4*100/23;
        return B;
    }
    
    public static float percentualDeUsoElevadorC() {
        float C = 9*100/23;
        return C;
    }
    
    public static float percentualDeUsoElevadorD() {
        float D = 1*100/23;
        return D;
    }
    
    public static float percentualDeUsoElevadorE() {
        float E = 1*100/23;
        return E;
    }
}
